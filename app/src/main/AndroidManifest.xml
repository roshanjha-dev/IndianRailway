<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rohandsouza.indianrailways">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.SEND_SMS"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/trainicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register" />
        <activity android:name=".Fetch_Announcement" />
        <activity android:name=".Home" />
        <activity android:name=".Locker" />
        <activity android:name=".feedback.Departments" />
        <activity android:name=".feedback.TrainNo" />
        <activity android:name=".feedback.List" />
        <activity android:name=".Locker_confirm" />
        <activity android:name=".OpenLocker" />
        <activity android:name=".PaypalImage"></activity>
    </application>

</manifest>